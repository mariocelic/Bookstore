Module Bookstore
{
    Entity Employees
    {
        ShortString Name;
        ShortString VAT { Unique; Required; }
    }

    Entity Manager
    {
        Extends Bookstore.Employees;

        ShortString Bonuses;
    }

    Entity EducationRecords
    {
        Reference Employees { Detail; }
        Date RecordDate;
        LongString Description;
    }
}